<?xml version='1.0' encoding='UTF-8'?>
<OWLSchema xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
	xsi:noNamespaceSchemaLocation='http://www.starlab.vub.ac.be/staff/mustafa/orm
	/verbalization/'> 


	<OWLNLMeta>
		<Meta name="DC.Title" content="Norwegian verbalization template"/>
		<Meta name="DC.Version" content="0.3"/>
		<Meta name="DC.Creator" content="Siri Holtnaes"/>
		<Meta name="DC.Language" content="Norwegian"/>
	</OWLNLMeta>

	<OWLNLBody>

		<Constraint xsi:type="SubClassOf">
			<Text> -[subsumption] </Text>
			<Object index="0"/>
			<Text>er en/t</Text>
			<Object index="1"/>
		</Constraint>

		<Constraint xsi:type="DisjointClasses">
			<Text> -[disjunction] </Text>
			<Object index="0"/>
			<Text> og </Text>
			<Object index="1"/>
			<Text>, er ikke det samme</Text>
		</Constraint>

		<Constraint xsi:type="ObjectSomeValuesFrom">
			<Text> -[existentialquantification] </Text>
			<Object index="0"/>
			<Text>minst en/t</Text>
			<Object index="1"/>
		</Constraint>

	</OWLNLBody>
</OWLSchema> 